---
layout: project
tab: community
---
.row-fluid.event
  .span8.offset2
    .title
      %h3 
        %a{:href=>page.event_url} #{page.title}
      %a{:href=>"https://github.com/jbosstools/jbosstools-website/edit/main#{page.relative_source_path}", :title=>"Edit on github"}
        %i.icon-edit
      %a{:href=>"https://github.com/jbosstools/jbosstools-website/commits/main#{page.relative_source_path}", :title=>"Show history on github"}
        %i.icon-github
      %p.info
        #{page.event_date}, #{page.location}
    .content
      ~content
    
